{
  "name": "kurtis-classrom",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src --max-warnings=0",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "schema:download": "apollo client:download-schema --endpoint=http://localhost:1337/graphql schema-temp.gql",
    "schema:convert": "node convertToNonNull.js",
    "codegen": "apollo codegen:generate --target typescript --excludes=node_modules/ --includes=src/graphql/**/*.ts --localSchemaFile schema-temp.gql --tagName=gql --outputFlat src/graphql/generate",
    "types:generate": "yarn schema:download && yarn schema:convert && yarn codegen",
    "generate": "yarn plop --plopfile generators/plopfile.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "postinstall": "husky install",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*": [
      "yarn lint --fix",
      "yarn test --findRelatedTests --bail"
    ]
  },
  "dependencies": {
    "dotenv": "^10.0.0",
    "graphql": "^15.6.1",
    "graphql-request": "^3.7.0",
    "next": "12.0.7",
    "next-pwa": "^5.3.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "styled-components": "5.3.3",
    "styled-media-query": "^2.1.2"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "6.4.9",
    "@storybook/builder-webpack5": "^6.4.9",
    "@storybook/manager-webpack5": "^6.4.9",
    "@storybook/react": "6.4.9",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.4.1",
    "@types/jest": "^27.0.2",
    "@types/node": "^17.0.5",
    "@types/react": "^17.0.30",
    "@types/styled-components": "^5.1.15",
    "@typescript-eslint/eslint-plugin": "5.8.1",
    "@typescript-eslint/parser": "5.8.1",
    "eslint": "^8.0.1",
    "eslint-config-next": "^12.0.7",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.2",
    "jest": "^27.2.5",
    "jest-styled-components": "^7.0.5",
    "lint-staged": "^12.1.4",
    "plop": "^3.0.5",
    "prettier": "^2.4.1",
    "storybook-addon-designs": "^6.2.0",
    "storybook-addon-next-router": "^3.0.8",
    "typescript": "^4.4.4",
    "webpack": "5"
  }
}
